@model IEnumerable<Person>

@foreach (var item in Model)
{
    <tr>
        <td class="cell-medium">
            @item.Name
        </td>
        <td class="cell-medium">@item.Age</td>
        <td class="cell-medium">@item.Location</td>
        <td class="cell-medium">@item.Experience</td>
        <td class="cell-medium">@item.EMail</td>
        <td class="cell-medium">@item.WebSite</td>
        <td class="cell-medium">@item.Degree</td>
        <td class="cell-medium">@item.Fax</td>
        <td class="cell-medium">@item.Careerlevel</td>
        <td class="cell-medium">@item.Phone</td>
        <td class="operation">
            <a asp-action="edit" asp-route-id="@item.Id" class="btn btn-warning">
                <i class="fa fa-pencil"></i>
            </a>
            <a asp-action="details" asp-route-id="@item.Id" class="btn btn-secondary">
                <i class="fa fa-eye"></i>
            </a>
            <a onclick="removeEntity(event,@item.Id,'@item.Name')" class="btn btn-danger">
                <i class="fa fa-trash"></i>
            </a>
        </td>
    </tr>
}



